<Window x:Class="MStore.Menu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MStore.Controls"
        mc:Ignorable="d"
        Title="Menu" Height="718" Width="1280" MinWidth="640" MinHeight="480">
	<Grid Background="{StaticResource BackgroundBrush}">
		<local:NavBar HorizontalAlignment="Left" Margin="10,8,0,0" VerticalAlignment="Top" Width="1248" RenderTransformOrigin="0.5,0.5">
			<local:NavBar.RenderTransform>
				<TransformGroup>
					<ScaleTransform/>
					<SkewTransform AngleY="-0.357"/>
					<RotateTransform/>
					<TranslateTransform Y="-1.494"/>
				</TransformGroup>
			</local:NavBar.RenderTransform>
		</local:NavBar>
		<TextBlock HorizontalAlignment="Left" Margin="382,297,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
	</Grid>
</Window>
